{
  "name": "localstore",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pkg-copy-bases": "cp ./server/config.base.json ./server/config.json && cp ./server/downloads.base.json ./server/downloads.json && cp ./bdd/localstore-db.base.json ./bdd/localstore-db.json",
    "pkg-osx": "electron-packager --prune=true --overwrite --out=../localSTORE-APPS --platform=darwin --icon=localstore.icns ./",
    "pkg-x32": "electron-packager --prune=true --overwrite --out=../localSTORE-APPS --platform=win32 --arch=ia32 --icon=localstore.ico ./",
    "pkg-x64": "electron-packager --prune=true --overwrite --out=../localSTORE-APPS --platform=win32 --arch=x64 --icon=localstore.ico ./",
    "build": "npm run patchFsExtra && npm run clean && npm run pkg-osx && npm run pkg-x32 && npm run pkg-x64",
    "patchFsExtra": "cp ./node_modules/fs-extra/lib/json/output-json-sync.js ./node_modules/fs-extra/lib/json/ls-json-sync.js && cp ./node_modules/fs-extra/lib/json/output-json.js ./node_modules/fs-extra/lib/json/ls-json.js && cp -R ./node_modules/fs-extra/lib/output ./node_modules/fs-extra/lib/ls-out",
    "clean": "rm -f localSTORE.xmb && npm run pkg-copy-bases"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.1.4"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "csvtojson": "^2.0.8",
    "electron-photon": "^1.1.6",
    "express": "^4.16.4",
    "fs-extra": "7.0.1",
    "jsonpath": "^1.0.1",
    "jsonpath-plus": "^0.18.0",
    "move-file": "^1.2.0",
    "ncp": "^2.0.0",
    "node-downloader-helper": "^1.0.10",
    "node-wget": "^0.4.3",
    "request": "^2.88.0",
    "unzipper": "^0.9.11",
    "whatwg-fetch": "^3.0.0",
    "ip": "^1.1.5"
  },
  "productName": "localstore"
}